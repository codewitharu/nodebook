<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mynotestory">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/note_dairy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/note_dairy"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyNoteStory">
        <activity android:name=".RemindMain"></activity>
        <activity android:name=".ReminderCreate" />
        <activity android:name=".UpdateActvity" />
        <activity android:name=".ActivityShow" />
        <activity android:name=".ActivityCreate" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>